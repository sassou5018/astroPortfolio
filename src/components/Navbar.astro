---
export interface props {
    title: string;
}

const {title} = Astro.props;
---
<div class="navbar bg-base-100">
    <div class="flex-1">
      <a class="btn btn-ghost normal-case text-xl">{title}</a>
    </div>
    <div class="flex-none">
      <ul class="menu menu-horizontal px-1 hidden sm:flex">
        <slot/>
      </ul>
    </div>
  </div>